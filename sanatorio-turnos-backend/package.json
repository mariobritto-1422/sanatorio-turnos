{
  "name": "sanatorio-turnos-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de gestión de turnos - Sanatorio Psiquiátrico",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "postbuild": "npm run db:deploy && npm run db:seed:prod",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma db push --skip-generate --accept-data-loss",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "keywords": ["turnos", "salud", "psiquiatria"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.16",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "prisma": "5.22.0",
    "twilio": "^5.3.4",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
  }
}
